{
  "name": "cocos-es6",
  "version": "0.0.1",
  "description": "Quick start for cocos 2d with es6",
  "main": "setup.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "node tools/setup.js",
    "start": "npm run dev",
    "dev": "webpack-dev-server --port 8080 --progress --colors --hot",
    "prod:bundlesrc": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "prod:cocos": "cocos deploy -p web -m release",
    "production": "npm run clean && npm run prod:bundlesrc && npm run prod:cocos && rm -f bundle.js",
    "clean": "rm -rf publish && rm -f bundle.js"
  },
  "keywords": [
    "cocos2d",
    "es6",
    "es2015",
    "babel",
    "webpack",
    "eslint",
    "boilerplate",
    "quickstart"
  ],
  "author": "Shidil Eringa",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "chai-immutable": "1.6.0",
    "eslint": "3.0.1",
    "eslint-loader": "1.4.1",
    "inquirer": "^1.1.2",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
